{
  "PurchaseSettings": {
    "BaseUrl": "https://zakupki.gov.ru/epz/order/extendedsearch/results.html?searchString={PHRASE}&morphology=on&search-filter=%D0%94%D0%B0%D1%82%D0%B5+%D1%80%D0%B0%D0%B7%D0%BC%D0%B5%D1%89%D0%B5%D0%BD%D0%B8%D1%8F&pageNumber={NUMBER}&sortDirection=false&recordsPerPage=_10&showLotsInfoHidden=false&sortBy=UPDATE_DATE&fz44=on&fz223=on&af=on&ca=on&pc=on&pa=on&currencyIdGeneral=-1",
    "PurchaseName": "бумага",
    "FirstPageNum": 1,
    "LastPageNum": 2
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      { "Name": "File", "Args": { "path": "Logs/log.txt" } },
      { "Name": "Console", "Args": { "stdoutLogEnabled": true, "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level}] {Message}{NewLine}{Exception}" } }
    ],
    "Enrich": [ "FromLogContext" ],
    "Destructure": [
      { "Name": "ToMaximumDepth", "Args": { "maximumDestructuringDepth": 4 } },
      { "Name": "ToMaximumStringLength", "Args": { "maximumStringLength": 100 } },
      { "Name": "ToMaximumCollectionCount", "Args": { "maximumCollectionCount": 10 } }
    ]
  },
  "MyRateLimitOptions" : {
      "PermitLimit" : 4,
      "Window" : 12,
      "QueueLimit" : 2
  },
  "AllowedHosts": "*"
}
